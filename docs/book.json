{
  "title": "TD-UI",
  "language": "zh-hans",
  "plugins": ["toc", "-lunr", "-search", "search-pro", "donate",
    "prism", "-highlight", "anchor-navigation-ex", "splitter"
  ],
  "pluginsConfig": {
    "sharing": {
      "weibo": true,
      "facebook": true,
      "twitter": true,
      "google": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook", "google", "twitter",
        "weibo", "instapaper"
      ]
    },
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": true,
      "associatedWithSummary": true,
      "mode": "float",
      "float": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "theme-default": {
      "showLevel": true
    },
    "donate": {
      "wechat": "https://hihl.github.io/weixin.png",
      "alipay": "https://hihl.github.io/alipay.png",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    }
  }
}
